/* programa que preencha uma matriz 3x3 com valores inteiros entre [1..9] */
#include <stdio.h>
int main(){
    
    int matriz[2][2];
    int i, j, aux;
    aux=0;
      for (i=0;i<3;i++){
        for (j=0;j<3;j++){
            aux++;
            matriz[i][j]=aux;
            printf("%d ", matriz[i][j]);
        }
        printf("\n");
      }
     return 0;
    } 
