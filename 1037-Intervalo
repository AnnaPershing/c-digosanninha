#include <stdio.h>

int main() {

double num; // vaiavel
scanf("%lf", &num); // leitura da variavel
if(num<0 || num>100.0) // intervalo fora
  printf("Fora de intervalo\n");
  else if(num<=25.0)
  printf("Intervalo [0,25]\n"); // indica intervalo de 0,25
   else if(num<=50.0)
   printf("Intervalo (25,50]\n");// indica intervalo de 25,50
    else if(num<=75.0)
    printf("Intervalo (50,75]");// indica intervalo de 50,75
      else if(num<=100.0)
      printf("Intervalo (75,100]\n"); // indica intervalo de 75,100
return 0;

}
