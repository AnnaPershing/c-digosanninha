#include <stdio.h>
#include <math.h>

int main() {
    double x1, y1, x2, y2; // variaveis
    scanf("%lf %lf %lf %lf", &x1, &y1, &x2, &y2); // leitura dos pontos
    pot1=pow(x2-x1, 2.0);
    pot2=pow(y2-y1, 2.0); // calculo
    printf("%.4lf\n", sqrt(pot1 + pot2)); // resposta da distancia entre pontos
    return 0;
}
