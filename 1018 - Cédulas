#include <stdio.h>

int main() {

int valor,troco;
int c100 , c50 , c20 , c10 , c5 , c2 ; // variaveis

scanf("%d", &valor); // leitura do valor

troco = valor; // calculos
c100 = troco / 100;
troco = troco - (c100 *100);
c50 = troco / 50;
troco = troco - (c50 * 50);
c20 = troco / 20;
troco = troco - (c20 * 20);
c10 = troco /10;
troco = troco -(c10 * 10);
c5 = troco / 5;
troco = troco - (c5 * 5);
c2 = troco / 2;
troco = troco - (c2 * 2);

printf("%d\n",valor); // impressoes
printf("%d nota(s) de R$ 100,00\n",c100);
printf("%d nota(s) de R$ 50,00\n",c50);
printf("%d nota(s) de R$ 20,00\n",c20);
printf("%d nota(s) de R$ 10,00\n",c10);
printf("%d nota(s) de R$ 5,00\n",c5);
printf("%d nota(s) de R$ 2,00\n",c2);
printf("%d nota(s) de R$ 1,00\n",troco);


return 0;

}
