#include<stdio.h>

int main(){
    
  int i, n, maior=0, posicao=0;
    for(i = 1 ; i <= 100; i++){ // vetor para leitura dos numeros
      scanf("%d", &n); // leitura dos numeros
      if(maior < n){ // selecao do maior
        maior = n;
        posicao = i;
      }
    }
    printf("%d\n%d\n",maior,posicao); // resposta do maior numero e sua posição
    
    return 0;
}
