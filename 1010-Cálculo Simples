#include <stdio.h>
int main() {
    int C1, C2, N1, N2;
    double V1, V2; // variaveis
    
    scanf ("%d %d %lf", &C1, &N1, &V1); // leitura primeira peça
    scanf ("%d %d %lf", &C2, &N2, &V2); // leitura segunda peça
    printf ("VALOR A PAGAR: R$ %.2f\n",(N1*V1)+(N2*V2));
    
    return 0;

}
