#include <stdio.h>

int main()
{
    int A, B, C, D; // variaveis
    scanf("%i %i %i %i", &A, &B, &C, &D); 
    if (B > C && D > A && C+D > A+B && C >= 0 && D >= 0 && A%2 == 0)// Condição pros valores aceitos
        printf("Valores aceitos\n");
  
    else
        printf("Valores nao aceitos\n"); // caso contrario, não aceitos
 
    return 0;
}
